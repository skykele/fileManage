{
  "name": "kele",
  "version": "1.1.0",
  "author": "可乐",
  "description": "文件搜索",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .electron-vue/dev-runner.js",
    "build:win32": "cross-env BUILD_TARGET=clean node .electron-vue/build.js  && electron-builder --win  --ia32",
    "build:win64": "cross-env BUILD_TARGET=build node .electron-vue/build.js  && electron-builder --win  --x64",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "extraFiles": [],
    "publish": [
      {
        "provider": "generic",
        "url": "https://ff-cloud.oss-cn-beijing.aliyuncs.com/electron/down"
      }
    ],
    "productName": "future-electron",
    "appId": "org.future.electron",
    "extraResources": {
      "from": "./config.conf",
      "to": "../"
    },
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/icons/icon.ico",
      "uninstallerIcon": "./build/icons/icon.ico",
      "installerHeaderIcon": "./build/icons/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "文件搜索"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": "nsis"
    },
    "linux": {
      "target": "deb",
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "about-window": "^1.15.2",
    "axios": "^0.26.1",
    "dayjs": "^1.9.5",
    "echarts": "^5.3.2",
    "electron-better-ipc": "^2.0.1",
    "electron-context-menu": "^3.1.2",
    "electron-dl": "^3.3.1",
    "electron-log": "^4.2.2",
    "electron-pdf-window": "^1.0.12",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.0.1",
    "electron-util": "^0.17.2",
    "encryptlong": "^3.1.4",
    "express": "^4.17.3",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.1",
    "fuse.js": "^6.4.3",
    "jsencrypt": "3.0.0-rc.1",
    "lodash": "^4.17.21",
    "lodash-id": "^0.14.1",
    "lowdb": "^1.0.0",
    "menubar": "^9.2.0",
    "umy-ui": "^1.1.7",
    "v-viewer": "^3.0.21",
    "viewerjs": "^1.11.7",
    "vue-codemirror": "^4.0.6",
    "vue-count-to": "^1.0.13",
    "vue-cropper": "^0.5.8",
    "vue-meta": "^2.4.0",
    "xlsx": "^0.18.5",
    "zx-quill": "0.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/plugin-proposal-do-expressions": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-proposal-function-sent": "^7.16.7",
    "@babel/plugin-proposal-json-strings": "^7.16.7",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.17.6",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.8",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^14.14.41",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "ajv": "^6.5.0",
    "babel-loader": "^8.2.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "18.0.3",
    "electron-builder": "^23.0.3",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^3.2.0",
    "element-ui": "^2.15.8",
    "esbuild-loader": "^2.18.0",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "listr": "^0.14.3",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "sass": "1.32.13",
    "sass-loader": "10.1.1",
    "script-loader": "^0.7.2",
    "style-loader": "^3.3.1",
    "svg-sprite-loader": "^6.0.11",
    "url-loader": "^4.1.1",
    "vue": "^2.6.14",
    "vue-html-loader": "^1.2.4",
    "vue-i18n": "^8.27.1",
    "vue-loader": "^15.9.8",
    "vue-router": "3.4.9",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vuex": "3.6.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
